    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:width="10dp"
        android:height="280dp"
        android:background="@color/black"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/tabuleiro" />

    <TextView
        android:id="@+id/tab1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="87dp"
        android:height="70dp"
        android:onClick="tab1_1"
        app:layout_constraintStart_toStartOf="@+id/tabuleiro"
        app:layout_constraintTop_toTopOf="@+id/tabuleiro"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/tab1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:width="87dp"
        android:height="70dp"
        app:layout_constraintStart_toStartOf="@+id/tabuleiro"
        app:layout_constraintTop_toTopOf="@+id/tabuleiro" />

    <TextView
        android:id="@+id/tab1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="192dp"
        android:width="87dp"
        android:height="70dp"
        app:layout_constraintStart_toStartOf="@+id/tabuleiro"
        app:layout_constraintTop_toTopOf="@+id/tabuleiro" />

    <TextView
        android:id="@+id/tab2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:width="87dp"
        android:height="100dp"
        app:layout_constraintStart_toStartOf="@+id/tabuleiro"
        app:layout_constraintTop_toTopOf="@+id/tabuleiro" />

    <TextView
        android:id="@+id/tab2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="80dp"
        android:width="87dp"
        android:height="100dp"
        app:layout_constraintStart_toStartOf="@+id/tabuleiro"
        app:layout_constraintTop_toTopOf="@+id/tabuleiro" />

